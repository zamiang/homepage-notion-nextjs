{
  "name": "brennanmoore-nextjs-notion-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "audit-dependencies": "npm audit --audit-level=high --production",
    "build": "next build",
    "cache:posts": "tsx scripts/cache-posts.ts",
    "dev": "next dev",
    "format": "prettier --write \"{src,__tests__,scripts,.claude,docs}/**/*.{ts,tsx,js,css,md}\"",
    "format:check": "prettier --list-different \"{src,__tests__,scripts,docs}/**/*.{ts,tsx,js,css,md}\"",
    "lint": "eslint . --ext .ts,.tsx --max-warnings=0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "start": "next start",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:watch:coverage": "vitest --watch --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@notionhq/client": "5.4.0",
    "@radix-ui/react-label": "2.1.8",
    "@radix-ui/react-slot": "1.2.4",
    "@vercel/analytics": "1.5.0",
    "@vercel/speed-insights": "1.2.0",
    "axios": "1.13.2",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "express": "5.1.0",
    "lucide-react": "0.553.0",
    "next": "16.0.3",
    "next-secure-headers": "2.2.0",
    "next-themes": "0.4.6",
    "notion-to-md": "3.1.9",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-markdown": "10.1.0",
    "rehype-raw": "7.0.0",
    "remark-gfm": "4.0.1",
    "sitemap": "9.0.0",
    "slugify": "1.6.6",
    "tailwind-merge": "3.4.0",
    "tailwindcss": "4.1.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.39.1",
    "@tailwindcss/postcss": "4.1.17",
    "@tailwindcss/typography": "0.5.19",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@trivago/prettier-plugin-sort-imports": "6.0.0",
    "@types/express": "5.0.5",
    "@types/node": "24.10.1",
    "@types/react": "19.2.5",
    "@types/react-dom": "19.2.3",
    "@types/react-syntax-highlighter": "15.5.13",
    "@typescript-eslint/parser": "8.46.4",
    "@vitejs/plugin-react": "5.1.1",
    "@vitest/coverage-istanbul": "4.0.12",
    "@vitest/ui": "4.0.9",
    "dotenv": "17.2.3",
    "eslint": "9.39.1",
    "eslint-config-next": "16.0.3",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.4",
    "jsdom": "27.2.0",
    "prettier": "3.6.2",
    "react-syntax-highlighter": "16.1.0",
    "tsx": "4.20.6",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.4",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "4.0.9"
  },
  "engines": {
    "node": "22.x"
  }
}
